<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sensorweb.datacenter.dao.PhenomenonMapper">
    <sql id="insertFields">
        phenomenon_id, phenomenon_description, unit
    </sql>

    <sql id="selectFields">
        phenomenon_id, phenomenon_description, unit
    </sql>

    <insert id="insertData" parameterType="Phenomenon">
        INSERT INTO phenomenon (<include refid="insertFields"></include>)
        VALUES (#{id}, #{description}, #{unit})
    </insert>

</mapper>